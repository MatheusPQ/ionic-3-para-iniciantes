// Telefone: 99999999
// Enderecos: 
// 	Casa => Rua 1
// 	Trabalho => Rua 2
// Profissao: Desenvolvedor


// -- CONOTAÇÃO DE OBJETO --
// ---- Chaves geram objetos. Podem ter um número, um texto etc.
// ---- Colchetes geram listas.
{
	idade: 27,
	nome: "Matheus Pinheiro",
	telefone: "99999999",
	enderecos: [ // Colchete indica que enderecos é uma lista. Chaves indicam um objeto.
		{ // Portanto, lista de objetos
			titulo: "casa",
			endereco: "Rua 1"
		},
		{
			titulo: "trabalho",
			endereco: "Rua 2"
		}
	],
	profissao: "Desenvolvedor"
}

